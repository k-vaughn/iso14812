// Diagram for roadway
digraph {
	graph [overlap=false rankdir=TB splines=true]
	node [fontname=Arial fontsize=12 margin=0 shape=none]
	edge [fontname=Arial fontsize=11]
	{
		rank=max
		roadway [label=<<TABLE BORDER="1" CELLBORDER="0" CELLSPACING="0" CELLPADDING="1"><TR><TD BGCOLOR="lightgray" ALIGN="CENTER" PORT="e">roadway</TD></TR></TABLE>> URL="../classes/itsVocabulary__roadway.md" margin=0]
	}
	subgraph cluster_associated {
		label="" style=invis
		Invis [label=<<TABLE BORDER="0" CELLBORDER="0" CELLSPACING="0" CELLPADDING="1"><TR><TD></TD></TR></TABLE>> margin=0 style=invis]
		travelledWay [label=<<TABLE BORDER="1" CELLBORDER="0" CELLSPACING="0" CELLPADDING="1"><TR><TD BGCOLOR="lightgray" ALIGN="CENTER">travelled way</TD></TR></TABLE>> URL="../travelled way/" margin=0 target=_top]
	}
	roadway -> Invis [style=invis]
	Invis -> travelledWay [style=invis]
	roadway -> travelledWay [label=" comprises 
 «some»  " arrowhead=normal style=solid]
}
