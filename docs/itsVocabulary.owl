        <dcterms:modified rdf:datatype="xsd:date">2026-01-05</dcterms:modified>  <!-- latest edits -->

        <!-- Version history -->
        <owl:versionInfo>1.1.1</owl:versionInfo>
        <rdfs:comment>
            1.0.0 - 2022: Published as ISO/TS 14812:2022.
            1.1.0 - 2025: Updated to reflect ISO/TS 14812:2025 (Edition 2).
            1.1.1 - 2026: Converted to formal ontology.

            Note: Version numbers follow semantic versioning principles (major.minor.patch). This is
            different than ISO standard edition numbers, which are sequential.
        </rdfs:comment>
        <owl:versionIRI rdf:resource="https://w3id.org/itsVocab/1.1.1/" />
